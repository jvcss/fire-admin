{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    // "rootDir": "./",
    // "moduleResolution": "node10",
    // "baseUrl": "./",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    // "rootDirs": [],
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true /* Skip type checking all .d.ts files. */
  }
}